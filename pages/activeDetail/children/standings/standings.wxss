.standings-wrap {
  padding: 30rpx 0rpx;
  background: #f3f3f3;
}
.standing-inner {
  padding: 20rpx;
  margin-top: 30rpx;
  border-radius: 10rpx;
  background: #fff url('data:image/png;base64,') 93% center no-repeat;
  background-size: 176rpx 252rpx;
}
.standings-wrap .top image {
  width: 32rpx;
  height: 36rpx;
  margin-right: 20rpx;
}
.standings-wrap .center {
  margin-top: 36rpx;
  padding-left: 20rpx;
}
.standings-wrap .center > view {
  margin-right: 50rpx;
}
.standings-wrap .center > view .desc {
  margin-top: 14rpx;
}
.standings-wrap .center .num, .standings-wrap .center .rate {
  font-size: 40rpx;
}
.standings-wrap .center .num {
  color: #00afc0;
}
.standings-wrap .check-btn {
  margin-top: 36rpx;
  border-radius: 36rpx;
  background: linear-gradient(180deg, rgba(255, 169, 17, 1) 0%,
   rgba(254, 111, 22, 1) 100%);
  color: #fff;
}