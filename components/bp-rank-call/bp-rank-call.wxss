.flag-list .flag-item {
  padding: 4rpx 4rpx;
  margin-left: 10rpx;
  font-size: 18rpx;
  border: 2rpx solid transparent;
  border-top-left-radius: 15rpx;
  border-bottom-right-radius: 15rpx;
  opacity: 0.5;
  display: flex;
  align-items: center;
}
.flag-list .flag-item .iconfont {
  font-size: 18rpx;
  margin-right: 4rpx;
}
.flag-list .flag-item text {
  line-height: 1;
}
.flag-list .flag-item.star {  /* 星星 */
  color: #8D8D8D;
  border-color: #8D8D8D; 
}
.flag-list .flag-item.moon {  /* 月亮 */
  color: #0BC1A2;
  border-color: #0BC1A2; 
}
.flag-list .flag-item.sun {  /* 太阳 */
  color: #FF8615;
  border-color: #FF8615; 
}
.flag-list .flag-item.zuanshi {  /* 钻石 */
  color: #6E16CA;
  border-color: #6E16CA; 
}
.flag-list .flag-item.huangguan {  /* 皇冠 */
  color: #FF4233;
  border-color: #FF4233; 
}
.flag-list .flag-item.group {  /* 群 */
  color: #7fd7e0;
  border-color: #7fd7e0; 
}
.flag-list .call {
  position: relative;
  height: 26rpx;
  padding-left: 28rpx;
  padding-right: 10rpx; 
  margin-left: 10rpx;
  border-radius: 40rpx;
  font-size: 16rpx;
  color: #fff;
}
.flag-list .call .call-icon {
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -15rpx;
  width: 25rpx;
  height: 30rpx;
}
.flag-list .call text {
  display: inline-block;
  height: 100%;
  line-height: 26rpx;
}
.host {
  margin-left: 10rpx;
  font-size: 24rpx;
}
.flag-list .fans {
  width: 67rpx;
  height: 26rpx;
  padding-left: 30rpx;
  margin-left: 7rpx;
  font-size: 16rpx;
  line-height: 26rpx;
  color: #fff;
  background: url('data:image/png;base64,');
  background-size: 100% 100%;
}